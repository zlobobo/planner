<table class="table">
  <tr>
    <th>Name</th>
    <th>Actions</th>
  </tr>

<% @activity_categories.each do |activity_category| %>
  <tr>
    <td><%= activity_category.name %></td>
    <td><%= link_to 'Edit', edit_activity_category_path(activity_category) %> <%= link_to 'Destroy', activity_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>




